{
	"name": "every-last-drop",
	"version": "1.0.0",
	"description": "Every Last Drop is the complete database management system for the small bar, or bar owner",
	"main": "src/app.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"start": "node .",
		"sql-setup": "run-s gen-sql gen-transaction-sql genSqlUser sql-init sql-insert-data sql-insert-transactions",
		"sql-init": "node src/dev/run-script-os/run-script-os-custom-index.js sql-init",
		"sql-init:linux": "sh src/build/linux-sql-init.sh",
		"sql-init:darwin": "sh src/build/darwin-sql-init.sh",
		"sql-init:default": "src\\build\\windows-sql-init.bat",
		"sql-login": "node src/dev/run-script-os/run-script-os-custom-index.js sql-login",
		"sql-login:linux": "sh src/dev/linux-sql-login.sh",
		"sql-login:darwin": "sh src/dev/darwin-sql-login.sh",
		"sql-login:default": "src\\dev\\windows-sql-login.bat",
		"sql-insert-data": "node src/dev/run-script-os/run-script-os-custom-index.js sql-insert-data",
		"sql-insert-data:darwin": "sudo mariadb -u root -p < data/sample-data.sql",
		"sql-insert-data:linux": "sh src/dev/linux-sql-upload.sh data/sample-data.sql",
		"sql-insert-data:default": "src\\dev\\windows-sql-upload.bat data\\sample-data.sql",
		"sql-insert-transactions": "node src/dev/run-script-os/run-script-os-custom-index.js sql-insert-data",
		"sql-insert-transactions:darwin": "sudo mariadb -u root -p < data/transaction-data.sql",
		"sql-insert-transactions:linux": "sh src/dev/linux-sql-upload.sh data/transaction-data.sql",
		"sql-insert-transactions:default": "src\\dev\\windows-sql-upload.bat data\\transaction-data.sql",
		"scrape-data": "echo '[' > data/sample-data.json ; node src/dev/cocktailDataCollector.js >> data/sample-data.json; truncate -s-2 data/sample-data.json ; echo ']' >> data/sample-data.json ; node src/dev/cocktailDataFormatter.js data/sample-data.json",
		"gen-transactions": "node src/dev/transactionGenerator.js 500 100 data/transaction-data.json ./data/sample-data.json",
		"gen-transaction-sql": "node src/dev/jsonToSql.js ./data/transaction-data.json ./data/transaction-data.sql",
		"gen-sql": "node src/dev/jsonToSql.js ./data/sample-data.json ./data/sample-data.sql",
		"dev": "run-p watch dev:server",
		"dev:server": "nodemon .",
		"watch": "webpack --config src/webpack.config.js --watch",
		"build:production": "sed -i 's/development/production/' src/webpack.config.js ; npm run build:frontend ; sed -i 's/production/development/' src/webpack.config.js ;",
		"build:frontend": "webpack --config src/webpack.config.js",
		"project-setup": "run-s genAuth sql-setup build:frontend",
		"genSqlUser": "node src/build/generateSqlUser.js",
		"genAuth": "node src/build/generateEnvCredentials.js",
		"generate-docs": "rm -rf documentation/ ; jsdoc -d documentation/ src/app.js src/*/*.js src/frontend/*/*.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/brennanwilkes/every-last-drop.git"
	},
	"author": "Brennan Wilkes",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/brennanwilkes/every-last-drop/issues"
	},
	"homepage": "https://github.com/brennanwilkes/every-last-drop#readme",
	"dependencies": {
		"@babel/core": "^7.12.3",
		"@babel/preset-env": "^7.12.1",
		"@babel/preset-react": "^7.12.1",
		"axios": "^0.21.0",
		"babel-loader": "^8.1.0",
		"bootstrap": "^5.0.0-alpha2",
		"css-loader": "^5.0.0",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"jquery": "^3.5.1",
		"mariadb": "^2.4.2",
		"moment": "^2.29.1",
		"node": "^14.13.0",
		"popper.js": "^1.16.1",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-icons": "^3.11.0",
		"style-loader": "^2.0.0"
	},
	"devDependencies": {
		"file-loader": "^6.2.0",
		"nodemon": "^2.0.5",
		"npm-run-all": "^4.1.5",
		"url-loader": "^4.1.1",
		"webpack": "^5.3.2",
		"webpack-cli": "^4.1.0"
	}
}
