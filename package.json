{
	"name": "every-last-drop",
	"version": "1.0.0",
	"description": "Every Last Drop is the complete database management system for the small bar, or bar owner",
	"main": "src/app.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"start": "node .",
		"sql-init": "node src/dev/run-script-os/run-script-os-custom-index.js sql-init",
		"sql-init:linux": "sh src/build/linux-sql-init.sh",
		"sql-init:darwin": "sh src/build/darwin-sql-init.sh",
		"sql-init:default": "src\\build\\windows-sql-init.bat",
		"sql-login": "node src/dev/run-script-os/run-script-os-custom-index.js sql-login",
		"sql-login:linux": "sh src/dev/linux-sql-login.sh",
		"sql-login:darwin": "sh src/dev/darwin-sql-login.sh",
		"sql-login:default": "src\\dev\\windows-sql-login.bat",
		"gen-m3-sql": "node src/dev/jsonToSql.js ./data/sample-m3.json ./data/sample-m3.sql",
		"sql-status": "node src/dev/run-script-os/run-script-os-custom-index.js sql-status",
		"sql-status:linux:darwin": "mariadb --defaults-extra-file=config/sql-login --table < src/sql/status.sql",
		"sql-status:default": "\"\"\\Program Files\\MariaDB 10.5\\bin\\mariadb.exe\"\" --defaults-extra-file=config\\sql-login --table < src\\sql\\status.sql",
		"dev": "nodemon ."
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/brennanwilkes/every-last-drop.git"
	},
	"author": "Mali Truong/Charles Lewis/Brennan Wilkes",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/brennanwilkes/every-last-drop/issues"
	},
	"homepage": "https://github.com/brennanwilkes/every-last-drop#readme",
	"dependencies": {
		"express": "^4.17.1",
		"mariadb": "^2.4.2",
		"node": "^14.13.0"
	},
	"devDependencies": {
		"nodemon": "^2.0.5"
	}
}
